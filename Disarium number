#include <stdio.h>
#include <math.h>
int main()
{
    int n, temp, count = 0, sum = 0, digit;
    scanf("%d", &n);
    temp = n;
    while (temp > 0)
    {
        count++;
        temp /= 10;
    }
    temp = n;
    while (temp > 0)
    {
        digit = temp % 10;
        sum += pow(digit, count);
        count--;
        temp /= 10;
    }
    if (sum == n)
        printf("Disarium Number");
    else
        printf("Not Disarium Number");
    return 0;
}
