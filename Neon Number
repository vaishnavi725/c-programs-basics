#include <stdio.h>
int main()
{
    int N, square, sum = 0, digit;
    scanf("%d", &N);
    square = N * N;
    while (square > 0)
    {
        digit = square % 10;
        sum += digit;
        square /= 10;
    }
    if (sum == N)
        printf("Neon Number");
    else
        printf("Not Neon Number");
    return 0;
}
